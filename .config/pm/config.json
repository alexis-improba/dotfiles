{
  "global": {
    "colors": {
      "comment": "#6e738d",
      "error": "#ed8796",
      "success": "#a6da95",
      "suggestion": "#eed49f"
    },
    "home": "/Users/alexis/dev"
  },
  "spaces": {
    "default": "personal",
    "list": [
      "default",
      "advent-of-code",
      "personal",
      "epitech",
      "unifox",
      "tests"
    ]
  },
  "templates": {
    "cargo": [
      {
        "command": [
          "cargo",
          "new",
          "{project}"
        ],
        "dir": "SPACE"
      }
    ],
    "cobra-cli": [
      {
        "command": [
          "mkdir",
          "{project}"
        ],
        "dir": "SPACE"
      },
      {
        "command": [
          "go",
          "mod",
          "init",
          "github.com/alexis-moins/{project}"
        ],
        "dir": "PROJECT"
      },
      {
        "command": [
          "git",
          "init"
        ],
        "dir": "PROJECT"
      },
      {
        "command": [
          "cobra-cli",
          "init",
          "{project}"
        ],
        "dir": "PROJECT"
      }
    ],
    "default": [
      {
        "command": [
          "mkdir",
          "{project}"
        ],
        "dir": "SPACE"
      },
      {
        "command": [
          "git",
          "init"
        ],
        "dir": "PROJECT"
      }
    ],
    "golang": [
      {
        "command": [
          "mkdir",
          "{project}"
        ],
        "dir": "SPACE"
      },
      {
        "command": [
          "go",
          "mod",
          "init",
          "github.com/alexis-moins/{project}"
        ],
        "dir": "PROJECT"
      },
      {
        "command": [
          "git",
          "init"
        ],
        "dir": "PROJECT"
      }
    ],
    "laravel": [
      {
        "command": [
          "composer",
          "create-project",
          "laravel/laravel",
          "{project}"
        ],
        "dir": "SPACE"
      }
    ]
  }
}