{
  "global": {
    "home": "/Users/alexis/dev"
  },
  "spaces": {
    "default": "personal",
    "list": [
      "default",
      "playground",
      "personal",
      "three-js-journey"
    ]
  },
  "templates": {
    "cargo": [
      {
        "command": [
          "cargo",
          "new",
          "{project}"
        ],
        "dir": "SPACE"
      }
    ],
    "default": [
      {
        "command": [
          "mkdir",
          "{project}"
        ],
        "dir": "SPACE"
      },
      {
        "command": [
          "git",
          "init"
        ],
        "dir": "PROJECT"
      }
    ],
    "golang": [
      {
        "command": [
          "mkdir",
          "{project}"
        ],
        "dir": "SPACE"
      },
      {
        "command": [
          "go",
          "mod",
          "init",
          "github.com/alexis-moins/{project}"
        ],
        "dir": "PROJECT"
      },
      {
        "command": [
          "git",
          "init"
        ],
        "dir": "PROJECT"
      }
    ]
  }
}